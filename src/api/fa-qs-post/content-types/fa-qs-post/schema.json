{
  "kind": "collectionType",
  "collectionName": "fa_qs_posts",
  "info": {
    "singularName": "fa-qs-post",
    "pluralName": "fa-qs-posts",
    "displayName": "FAQs Post"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "fa_qs_category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::fa-qs-category.fa-qs-category",
      "inversedBy": "fa_qs_posts"
    },
    "Question": {
      "type": "blocks"
    },
    "Answer": {
      "type": "blocks"
    },
    "Author": {
      "type": "string"
    },
    "Icon": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },

    /* --- SEO Fields --- */
    "metaTitle": {
      "type": "string",
      "required": false,
      "default": ""
    },
    "metaDescription": {
      "type": "text",
      "required": false,
      "default": ""
    },
    "metaKeywords": {
      "type": "string",
      "required": false,
      "default": ""
    },
    "canonicalUrl": {
      "type": "string",
      "required": false,
      "default": ""
    },
    "ogTitle": {
      "type": "string",
      "required": false,
      "default": ""
    },
    "ogDescription": {
      "type": "text",
      "required": false,
      "default": ""
    },
    "ogImage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": ["images"]
    },
    "twitterTitle": {
      "type": "string",
      "required": false,
      "default": ""
    },
    "twitterDescription": {
      "type": "text",
      "required": false,
      "default": ""
    },
    "twitterImage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": ["images"]
    }
  }
}
